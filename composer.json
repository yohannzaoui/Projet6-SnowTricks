{
	"type" : "project",
	"license" : "proprietary",
	"require" : {
		"php" : "^7.1.3",
		"ext-ctype" : "*",
		"ext-iconv" : "*",
		"sensio/framework-extra-bundle" : "^5.2",
		"symfony/apache-pack" : "^1.0",
		"symfony/console" : "*",
		"symfony/flex" : "^1.1",
		"symfony/form" : "*",
		"symfony/framework-bundle" : "*",
		"symfony/messenger" : "*",
		"symfony/monolog-bundle" : "^3.3",
		"symfony/orm-pack" : "^1.0",
		"symfony/security-bundle" : "*",
		"symfony/swiftmailer-bundle" : "^3.2",
		"symfony/twig-bundle" : "*",
		"symfony/validator" : "*",
		"symfony/web-server-bundle" : "*",
		"symfony/yaml" : "*"
	},
	"require-dev" : {
		"symfony/dotenv" : "*",
		"symfony/web-profiler-bundle" : "*"
	},
	"config" : {
		"preferred-install" : {
			"*" : "dist"
		},
		"sort-packages" : true
	},
	"autoload" : {
		"psr-4" : {
			"App\\" : "src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"App\\Tests\\" : "tests/"
		}
	},
	"replace" : {
		"paragonie/random_compat" : "*",
		"symfony/polyfill-ctype" : "*",
		"symfony/polyfill-iconv" : "*",
		"symfony/polyfill-php71" : "*",
		"symfony/polyfill-php70" : "*",
		"symfony/polyfill-php56" : "*"
	},
	"conflict" : {
		"symfony/symfony" : "*"
	},
	"extra" : {
		"symfony" : {
			"allow-contrib" : true,
			"require" : "4.1.*"
		}
	},
	"name" : "Yohann zaoui",
	"description" : "Projet 6 OpenClassrooms"
}