<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                   https://raw.github.com/doctrine/doctrine2/master/doctrine-mapping.xsd">

    <entity name="App\Entity\Group" table="group">

        <id name="id" type="integer" column="id">
            <generator strategy="AUTO"/>
        </id>

        <field name="nameGroup" column="name_group" type="string" length="255" nullable="false" />
        <field name="description" column="description" type="text" nullable="true" />
        

        <many-to-one field="trick" target-entity="App\Entity\Trick" inversed-by="group">
        <join-column name="trick_id" referenced-column-name="id" />
        </many-to-one>

    </entity>


</doctrine-mapping>